{
    "name": "tienvx/single-color-petrinet",
    "type": "library",
    "description": "Single Color Petrinet framework for PHP 7.4+",
    "keywords": ["petrinet", "single-color-petrinet"],
    "homepage": "https://github.com/tienvx/single-color-petrinet",
    "license": "MIT",
    "authors": [
        {
            "name": "Tien Vo Xuan",
            "email": "tien.xuan.vo@gmail.com",
            "homepage": "https://tienvx.github.io/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "florianv/petrinet": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/expression-language": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "SingleColorPetrinet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SingleColorPetrinet\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
